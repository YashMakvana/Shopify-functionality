<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script>
    let currentQuestion = 1;
    const totalQuestions = 10; // Change this to total number of questions
    let selectedOptions = {};

    function startQuiz() {
        const name = document.getElementById('name').value;
        const mobile = document.getElementById('mobile').value;
        const email = document.getElementById('email').value;

        // Validate mobile number
        //   const mobilePattern = /^[0-9]{10}$/;
        //   if (!mobilePattern.test(mobile)) {
        //     alert('Please enter a valid 10-digit mobile number.');
        //     return;
        //   }

        // Validate email
        const emailPattern = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
        if (!emailPattern.test(email)) {
            alert('Please enter a valid email address.');
            return;
        }

        if (name.trim() === '' || mobile.trim() === '' || email.trim() === '') {
            alert('Please fill all fields.');
            return;
        }
        document.getElementById('startQuizForm').style.display = 'none';
        document.getElementById('startQA').style.display = 'block';
        showQuestion(currentQuestion);
        document.getElementById('nextBtn').style.display = 'inline-block';
    }

    function showQuestion(questionNumber) {
        document.querySelectorAll('.qa-box').forEach(function (question) {
            question.style.display = 'none';
        });
        document.getElementById('question' + questionNumber).style.display = 'block';
        if (questionNumber === 1) {
            document.getElementById('prevBtn').style.display = 'none';
        } else {
            document.getElementById('prevBtn').style.display = 'inline-block';
        }
        if (questionNumber === totalQuestions) {
            document.getElementById('nextBtn').style.display = 'none';
            document.getElementById('submitBtn').style.display = 'inline-block';
            document.getElementById('qa-email-checkbox').style.display = 'block';
            toggleNextButton(); // Enable submit button initially
        } else {
            document.getElementById('nextBtn').style.display = 'inline-block';
            document.getElementById('submitBtn').style.display = 'none';
            document.getElementById('qa-email-checkbox').style.display = 'none';
        }
    }

    // function toggleNextButton() {
    //   let selectedOption = document.querySelector('input[name="question' + currentQuestion + '"]:checked');
    //   if (selectedOption || currentQuestion !== totalQuestions) {
    //     document.getElementById('nextBtn').disabled = false;
    //   } else {
    //     document.getElementById('nextBtn').disabled = true;
    //   }
    //   // Check if it's the last question and an option is selected
    //   if (currentQuestion === totalQuestions && selectedOption) {
    //     document.getElementById('submitBtn').disabled = false;
    //   }
    // }


    function toggleNextButton() {
        document.getElementById('nextBtn').disabled = false;
        let selectedOption = document.querySelector('input[name="question' + currentQuestion + '"]:checked');
        if (!selectedOption) {
            document.getElementById('nextBtn').disabled = true;
        }
        // Check if it's the last question and an option is selected
        if (currentQuestion === totalQuestions && selectedOption) {
            document.getElementById('submitBtn').disabled = false;
        }
    }

    function prevQuestion() {
        if (currentQuestion > 1) {
            currentQuestion--;
            showQuestion(currentQuestion);
            toggleNextButton(); // Call toggleNextButton when going to previous question
        }
    }

    function nextQuestion() {
        let selectedOption = document.querySelector('input[name="question' + currentQuestion + '"]:checked');
        if (!selectedOption && currentQuestion !== totalQuestions) {
            alert('Please select an option before proceeding.');
            return;
        }
        if (currentQuestion < totalQuestions) {
            currentQuestion++;
            showQuestion(currentQuestion);
            toggleNextButton(); // Call toggleNextButton when going to next question
            // document.getElementById('nextBtn').disabled = true; // Disable next button again
        }
    }

    function submitQuiz() {
        // Check if the user has selected an option in the last question
        if (currentQuestion === totalQuestions) {
            let selectedOption = document.querySelector('input[name="question' + currentQuestion + '"]:checked');
            if (!selectedOption) {
                alert('Please select an option before submitting.');
                return;
            }
        }

        // Count selected options
        let selectedOptions = {
            'A': 0,
            'B': 0,
            'C': 0,
            'D': 0,
            'E': 0
        };

        for (let i = 1; i <= totalQuestions; i++) {
            let questionName = 'question' + i;
            let selectedOption = document.querySelector('input[name="' + questionName + '"]:checked');
            if (selectedOption) {
                let optionValue = selectedOption.value;
                selectedOptions[optionValue]++;
            }
        }

        // Prepare HTML for selected options and their counts
        let selectedOptionsHtml = '';

        let optionsInfo = {
            'A': 'Citrus',
            'B': 'Fresh',
            'C': 'Floral',
            'D': 'Woody',
            'E': 'Oriental'
        };

        let optionDescriptions = {
            'A': 'Cheerful, bright, extroverted, innocent & energetic people, who often like scent of fruits & fresh things, Young at heart & happy personality',
            'B': 'Happy go lucky, team players, who like taking calculated risks & are natural entrepreneur Functional extroverts & easy-going!',
            'C': 'Your feelings and ability to connect with people are intense, love people & things passionately & feel life very deeply, Emotional, romantic and sentimental',
            'D': 'Sophisticated, elegant & often deep-thinking, reserve & private nature, Like deeper darker, woody scents. Sexy & Mysterious personality',
            'E': 'Charismatic, bold, dramatic, flamboyant & unique, oriental fragrance lovers & who loves spices, vanilla and exotic florals, Glamorous & Exotic'
        };

        var answerdata = [];
        for (const option in selectedOptions) {
            selectedOptionsHtml += `
          <div class="qa-attempt-li">
            <span class="at-l">${option} - ${optionsInfo[option]}</span>
            <span class="at-mid">${optionDescriptions[option]}</span>
            <span class="at-r">${selectedOptions[option]}</span>
          </div>
        `;
            answerdata.push({
                answername: option,
                answertitle: optionsInfo[option],
                answerdescription: optionDescriptions[option],
                answerselvalue: selectedOptions[option]
            });
        }
        console.log(answerdata);
        // Insert selected options HTML into the container
        document.getElementById('selectedOptions').innerHTML = selectedOptionsHtml;

        // Show thank you message and user details
        const name = document.getElementById('name').value;
        const mobile = document.getElementById('mobile').value;
        const email = document.getElementById('email').value;
        document.getElementById('qa-thanks').style.display = 'block';
        document.getElementById('qa-user-nm').innerHTML += name;
        document.getElementById('qa-user-mobile').innerHTML += mobile;
        document.getElementById('qa-user-email').innerHTML += email;
        document.getElementById('question' + totalQuestions).style.display = 'none'; // Hide last question
        document.getElementById('startQA').style.display = 'none'; // Hide all question
        document.getElementById('prevBtn').style.display = 'none'; // Hide previous button
        document.getElementById('submitBtn').style.display = 'none'; // Hide submit button


        if (document.getElementById('qa-email').checked) {
            console.log('Email sent');
            $.ajax({
                method: "POST",
                dataType: "json",
                // headers: {'Access-Control-Allow-Origin': '*'},
                url: "https://scentopia.technoexponent.net/mailer/index.php",
                data: {
                    username: name,
                    useremail: email,
                    usermobile: mobile,
                    answerdata
                },
                // success: function(){
                //   console.log('success');
                // },
                // error: function(){
                //   console.log('failure');
                // }
            });
        }
    }



</script>
