                    <button id="prevBtn" class="qa-btn" style="display: none;"
                        onclick="prevQuestion()">Previous</button>
                    <button id="nextBtn" class="qa-btn" style="display: none; margin-left: auto;"
                        onclick="nextQuestion()" disabled>Next</button>
                    <button id="submitBtn" class="qa-btn" style="display: none;" onclick="submitQuiz()"
                        disabled>Submit</button>
